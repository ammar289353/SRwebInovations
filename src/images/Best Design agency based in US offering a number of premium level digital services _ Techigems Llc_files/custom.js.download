        t.from(".cross-wrapper", { scale: 0, ease: "elastic", duration: 0.5 }),
        s.addEventListener("click", function () {
            t.play(), t.timeScale(2);
        });
    i.addEventListener("click", function () {
        t.reverse();
    });
    let r = document.querySelectorAll(".home-banner");
